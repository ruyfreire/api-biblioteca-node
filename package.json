{
  "name": "api-biblioteca-node",
  "version": "1.0.0",
  "description": "API de uma biblioteca em nodejs",
  "main": "index.js",
  "author": "Ruy Freire",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --exit-child src/index.ts",
    "test": "env-cmd -f .env.test jest --verbose --forceExit --detectOpenHandles --maxWorkers=1",
    "test:migrate": "env-cmd -f .env.test prisma migrate dev && prisma generate",
    "test:migrate:ci": "prisma migrate dev && prisma generate",
    "test:ci": "jest --verbose --forceExit --detectOpenHandles --maxWorkers=1",
    "build": "tsc",
    "lint": "eslint --fix src/**/*.ts",
    "prettier": "prettier --write src/**/*.ts",
    "prisma:studio": "prisma studio",
    "prisma:migrate_dev": "prisma migrate dev",
    "prisma:pull_db": "prisma db pull"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@faker-js/faker": "^7.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.4.1",
    "prisma": "^3.14.0",
    "supertest": "^6.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.4.4"
  }
}
